     /*Name1*/
     public static String getName1(String sf_name){
    	 
    	 int lengthOfSFName = sf_name.length();
    	 String Name1="";
    	 System.out.println("Name1 method");
    	 
    	 if(lengthOfSFName == 0)
    	 {
    	        Name1 = "";
    	        //System.out.println(Name1);
    	        return Name1;
    	 }
    	 else if(lengthOfSFName <=35)
    	 {
    		 	Name1 = sf_name.substring(0, lengthOfSFName);
    		 	//System.out.println(Name1);
    		 	return Name1;
         }
    	 else 
    	 {
    		 	Name1 = sf_name.substring(0, 35);
    		 	//System.out.println(Name1);
    		 	return Name1;
         }  	 
    	 
     }
     
